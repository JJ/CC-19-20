          my $self = shift;
          my $old  = ${*$self}{myftp_home};
          if (@_) {
              ${*$self}{myftp_home} = shift;
          }
          $old;
      }
  
      sub go_home {
          my $self = shift;
          $self->cwd(${*$self}{myftp_home});
      }
  
      sub request_count {
          my $self = shift;
