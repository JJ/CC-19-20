          if (length($remote_file) and $data = $ftp->retr($remote_file)) {
              my ($type, @enc) = LWP::MediaTypes::guess_media_type($remote_file);
              $response->header('Content-Type', $type) if $type;
              for (@enc) {
                  $response->push_header('Content-Encoding', $_);
