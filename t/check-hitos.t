  require HTTP::Status;
  
  use base qw(LWP::Protocol);
  
  use HTTP::Date qw(time2str);
  require LWP;  # needs version number
  
  sub request
  {
      my($self, $request, $proxy, $arg, $size) = @_;
  
      # check proxy
      if (defined $proxy)
      {
  	return HTTP::Response->new( HTTP::Status::RC_BAD_REQUEST,
  				  'You can not proxy with data');
      }
  
